{
  "root": true,
  "ignorePatterns": ["node_modules", "dist", ".nx", "**/*.stories.ts", "**/*.spec.ts", "**/jest.config.ts"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "project": "./tsconfig.base.json"
      },
      "plugins": ["@typescript-eslint"],
      "rules": {}
    }
  ]
}
