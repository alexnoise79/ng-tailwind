{
  "name": "ng-tailwind",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nx build",
    "build:lib": "nx build ui-components",
    "prepublish:lib": "npm run build:lib && cp README.md dist/libs/ui-components/ && node -e \"const fs=require('fs');const pkg=JSON.parse(fs.readFileSync('dist/libs/ui-components/package.json'));delete pkg.scripts;fs.writeFileSync('dist/libs/ui-components/package.json',JSON.stringify(pkg,null,2));\"",
    "publish:lib": "npm run prepublish:lib && cd dist/libs/ui-components && npm publish --access=public",
    "publish:lib:dry-run": "npm run prepublish:lib && cd dist/libs/ui-components && npm publish --dry-run --access=public",
    "test": "nx run ui-components:test",
    "lint": "nx run-many --target=lint ",
    "lint:fix": "nx run-many --target=lint --fix",
    "format:write": "nx format:write --all",
    "serve": "nx serve",
    "storybook": "nx storybook ui-components"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
