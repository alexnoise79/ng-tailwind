    "@angular-eslint/template-parser": "^20.0.0",
    "@angular/build": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@cypress/webpack-dev-server": "^3.8.0",
    "@nx/angular": "^22.1.1",
    "@nx/cypress": "20.0.0",
    "@nx/eslint": "20.0.0",
    "@nx/storybook": "20.0.0",
    "@nx/vite": "^22.1.1",
    "@nx/vitest": "^22.1.1",
